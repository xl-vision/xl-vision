{
  "extends": ["config:base", "schedule:weekly"],
  "labels": ["dependencies"],
  "ignorePaths": [],
  "pin": true,
  "rangeStrategy": "bump",
  "node": false,
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "depTypeList": ["peerDependencies"],
      "enabled": false
    },
    {
      "matchPackageNames": ["react", "react-dom", "@types/react", "@types/react-dom", "csstype"],
      "enabled": false
    },
    {
      "matchDatasources": ["npm"],
      "stabilityDays": 3
    }
  ],
  "ignoreDeps": []
}
