{
  "extends": ["config:base", "schedule:weekly"],
  "labels": ["dependencies"],
  "ignorePaths": [],
  "pin": true,
  "rangeStrategy": "bump",
  "prConcurrentLimit": 5,
  "force": {
    "constraints": {
      "node": ">=14.19.0",
      "pnpm": ">=7"
    }
  },
  "packageRules": [
    {
      "matchDepTypes": ["peerDependencies"],
      "enabled": false
    },
    {
      "matchPackageNames": ["react", "react-dom", "@types/react", "@types/react-dom", "csstype"],
      "enabled": false
    },
    {
      "matchDatasources": ["npm"],
      "stabilityDays": 3
    }
  ],
  "ignoreDeps": []
}
