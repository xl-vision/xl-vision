  "repository": {
    "type": "git",
    "url": "git+https://github.com/xl-vision/xl-vision.git"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "cd platforms/docs && npm run dev",
    "docs": "cd platforms/docs && npm run docs",
    "start": "cd platforms/docs && npm run start",
    "icon:generate": "cd packages/icons && npm run generate",
    "commit": "git-cz",
    "test": "jest",
    "clean": "node scripts/clean.js",
    "lint": "eslint . --report-unused-disable-directives --ext .js,.ts,.jsx,.tsx --max-warnings 0",
    "build": "lerna run --parallel --scope \"@xl-vision/*\" build",
    "publish": "lerna publish prerelease --conventional-commits",
    "bootstrap": "lerna bootstrap",
    "npmCheck": "node scripts/npmCheck.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/format": "^13.1.0",
    "@types/enzyme": "^3.10.9",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.13",
