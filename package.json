  "homepage": "https://github.com/xl-vision/xl-vision-next#readme",
  "bugs": {
    "url": "https://github.com/xl-vision/xl-vision-next/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xl-vision/xl-vision-next.git"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "cd platforms/docs && npm run dev",
    "docs": "cd platforms/docs && npm run docs",
    "icon:generate": "cd packages/icons && npm run generate",
    "commit": "git-cz",
    "test": "jest",
    "lint": "eslint . --cache --report-unused-disable-directives --ext .js,.ts,.jsx,.tsx --max-warnings 0",
    "release:build": "lerna run --parallel --scope \"@xl-vision/*\" build",
    "release:version": "lerna version --exact --conventional-commits --no-push",
    "release:publish": "lerna publish from-package --dist-tag beta",
    "bootstrap": "lerna bootstrap"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^11.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "confusing-browser-globals": "^1.0.10",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lerna": "^3.22.1",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
