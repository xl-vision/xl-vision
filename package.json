    "gulp-typescript": "^5.0.1",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jest-styled-components": "^7.0.5",
    "lerna": "^4.0.0",
    "minimist": "^1.2.5",
    "npm-check": "^5.9.2",
    "prettier": "^2.3.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "styled-components": "^5.3.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
