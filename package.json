    "@testing-library/user-event": "^14.5.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "17.0.4",
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
