  "scripts": {
    "prepare": "husky",
    "dev": "cd platforms/docs && npm run dev",
    "dev-next": "cd platforms/docs-next && npm run dev",
    "docs": "cd platforms/docs && npm run docs",
    "docs:analyze": "cd platforms/docs && npm run analyze",
    "start": "cd platforms/docs && npm run start",
    "icon:generate": "cd packages/icons && npm run generate",
    "commit": "git-cz",
    "test": "jest",
    "clean": "node scripts/clean.js",
    "lint:all": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint": "lint-staged",
    "lint:fix": "cross-env LINT_FIX=true lint-staged --no-stash",
    "build": "pnpm --parallel --filter \"@xl-vision/*\" run build",
    "updateVersion": "node ./scripts/updateVersion",
    "release": "pnpm publish --filter ./packages"
  },
