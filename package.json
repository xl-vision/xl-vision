    "@testing-library/user-event": "^14.4.3",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "17.0.4",
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "@types/styled-components": "^5.1.30",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "chalk": "^4.1.2",
